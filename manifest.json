{
    "name": "Scheduled URL Opener",
    "description": "Open Links at Scheduled Times.",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16": "16.png",
        "48": "48.png",
        "128": "128.png"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage", "alarms", "tabs"]
}