{
    "name": "Scheduled Link Opener",
    "description": "Open Links at Scheduled Times.",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["background.js"]
        }
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": ["storage", "alarms"]
}